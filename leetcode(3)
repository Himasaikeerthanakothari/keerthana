import java.util.*;
class even_odd
{
public static void main(String args[])
{
ArrayList<Integer> num=new ArrayList<Integer>();
int num;
res[sizeofnum];
int index=0;
for(int i=0;i<num.size;i++)
{
if(num[i]%2==0)
{
 res[index]=num[i];
 index=index+2;
}
}
index=1;
for(int i=1;i<num.size;i++)
{
if(num[i]%2==1)
{
res[index]=num[i];
index=index+2;
}
}
System.out.print(num);
}
}
