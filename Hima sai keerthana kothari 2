class evenodd
{
public static void main(String args[])
{
int a[]={5,16,23,48,59,62};
System.out.println("Odd numbers are : ");
for(int i=0;i<a.length;i++)
{
if(a[i]%2==1)
{
System.out.println(a[i]);
}
}
System.out.println("Even numbers are : ");
for (int i=0;i<a.length;i++)
{
if(a[i]%2==0)
{
System.out.println(a[i]);
}
}
}
}
